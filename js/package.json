{
  "name": "ceresimaging-rej",
  "description": "Ceres Imaging, now with the power of Advanced Artificial AI Technology(TM)",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "vue-cli-service build --target lib --inline-vue --modern ./src/jupyterlab-extension.js",
    "watch": "vue-cli-service build --watch --target lib --inline-vue --modern ./src/jupyterlab-extension.js",
    "jupyterlab": "jupyter labextension link . --no-build && jupyter lab --watch",
    "lint": "vue-cli-service lint",
    "build:app": "vue-cli-service build ./src/app.js",
    "serve": "vue-cli-service serve ./src/app.js"
  },
  "main": "dist/extension-entry.js",
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/mainmenu": "^2.0.0",
    "@lumino/algorithm": "^1.2.3",
    "@lumino/coreutils": "^1.3.1",
    "@lumino/datagrid": "^0.5.2",
    "@lumino/disposable": "^1.3.1",
    "@lumino/widgets": "^1.5.0",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "core-js": "^2.6.5",
    "fast-geotiff": "^1.0.0-beta.4",
    "gdal-js": "^1.1.2",
    "geotiff": "^1.0.0-beta.6",
    "glfx": "0.0.4",
    "jsfeat": "0.0.8",
    "konva": "^3.4.1",
    "plotty": "^0.4.0",
    "save-file": "^2.3.1",
    "threads": "^1.0.0-beta.3",
    "vue": "^2.6.10",
    "vue-class-component": "^7.2.3",
    "vue-google-oauth2": "^1.4.0",
    "vue-konva": "^2.0.7",
    "vue-localstorage": "^0.6.2",
    "vue-property-decorator": "^8.4.1",
    "vue-resize": "^0.4.5",
    "vuetify": "^1.5.5",
    "vuex": "^3.0.1",
    "zip-array": "^1.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-service": "^3.9.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "material-design-icons-iconfont": "^5.0.1",
    "sass": "^1.22.9",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "typescript": "~3.8.3",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.4.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0",
    "worker-loader": "^2.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "rules": {
      "no-console": "off",
      "no-debugger": "off"
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 ChromeAndroid versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 Edge versions"
  ],
  "jupyterlab": {
    "extension": true
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ]
}
